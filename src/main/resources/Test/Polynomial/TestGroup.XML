<?xml version="1.0" encoding="UTF-8"?>
<Tests>
    <Test>
        <Input></Input> <!--  输入串  -->
        <HandleValidate>false</HandleValidate> <!--  用于和结果作比较，如果这里是false，那么代码结果应当也是false  -->
        <Validate></Validate> <!--  校验数组  -->
    </Test>
    <Test>
        <Input>1</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>1</Validate>
    </Test>
    <Test>
        <Input>x</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,1</Validate>
    </Test>
    <Test>
        <Input>-1</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>-1</Validate>
    </Test>
    <Test>
        <Input>-x</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,-1</Validate>
    </Test>
    <Test>
        <Input>1+x</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,1,1</Validate>
    </Test>
    <Test>
        <Input>1-x</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,1,-1</Validate>
    </Test>
    <Test>
        <Input>x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,0,0,1</Validate>
    </Test>
    <Test>
        <Input>x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>1,0,0,0,0</Validate>
    </Test>
    <Test>
        <Input>1+x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,1,0,1</Validate>
    </Test>
    <Test>
        <Input>1+x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>1,0,1,0,0</Validate>
    </Test>
    <Test>
        <Input>1-x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,1,0,-1</Validate>
    </Test>
    <Test>
        <Input>1-x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>-1,0,1,0,0</Validate>
    </Test>
    <Test>
        <Input>x+x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,0,1,1</Validate>
    </Test>
    <Test>
        <Input>x+x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>1,0,0,1,0</Validate>
    </Test>
    <Test>
        <Input>x-x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,0,1,-1</Validate>
    </Test>
    <Test>
        <Input>x-x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>-1,0,0,1,0</Validate>
    </Test>
    <Test>
        <Input>-x+x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,0,-1,1</Validate>
    </Test>
    <Test>
        <Input>-x+x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>1,0,0,-1,0</Validate>
    </Test>
    <Test>
        <Input>-x-x^2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>0,0,0,-1,-1</Validate>
    </Test>
    <Test>
        <Input>-x-x^-2</Input>
        <HandleValidate>true</HandleValidate>
        <Validate>-1,0,0,-1,0</Validate>
    </Test>
    <Test>
        <Input>^x</Input>
        <HandleValidate>false</HandleValidate>
        <Validate>0,1,1</Validate>
    </Test>
    <Test>
        <Input>1^</Input>
        <HandleValidate>false</HandleValidate>
        <Validate>0,1,1</Validate>
    </Test>
    <Test>
        <Input>^</Input>
        <HandleValidate>false</HandleValidate>
        <Validate>0,1,1</Validate>
    </Test>
    <Test>
        <Input>xx</Input>
        <HandleValidate>false</HandleValidate>
        <Validate>0,1,1</Validate>
    </Test>
    <Test>
        <Input>x^+</Input>
        <HandleValidate>false</HandleValidate>
        <Validate>0,1,1</Validate>
    </Test>
</Tests>